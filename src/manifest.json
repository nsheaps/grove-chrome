{
    "name": "Grove.io",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Grove.io's [unofficial] desktop client",
    "app":{
        "urls": [
            "*://*.grove.io/",
            "*://grove.io/"
        ],
        "launch": {
            "web_url": "https://grove.io/app/"
        }
    },
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }

}
